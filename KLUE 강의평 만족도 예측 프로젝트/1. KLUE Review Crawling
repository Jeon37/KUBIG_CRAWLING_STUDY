      "source": [
        "for i in range(len(link_data)):\n",
        "  link_url = link_data['link'].values[i]\n",
        "  time.sleep(2) \n",
        "  driver.get(url=link_url)                                        #링크 접속\n",
        "\n",
        "  #끝까지 스크롤\n",
        "  last_height = driver.execute_script(\"return document.body.scrollHeight\")\n",
        "  while True:\n",
        "    driver.execute_script(\"window.scrollTo(0, document.body.scrollHeight);\")\n",
        "    time.sleep(SCROLL_PAUSE_SEC)\n",
        "    new_height = driver.execute_script(\"return document.body.scrollHeight\")\n",
        "    if new_height == last_height:\n",
        "        break\n",
        "    last_height = new_height\n",
        "\n",
        "  #강의명/교수명 추출\n",
        "  lec = driver.find_element_by_xpath('//*[@id=\"root\"]/div/div/section[1]/div/div[1]/div/div[2]/p[1]')\n",
        "  lec_name = lec.text\n",
        "  prof = driver.find_element_by_xpath('//*[@id=\"root\"]/div/div/section[1]/div/div[1]/div/div[2]/p[2]')\n",
        "  prof_name = prof.text\n",
        "\n",
        "  #다른 변수 추출\n",
        "  for j in range(1,200):\n",
        "    try:\n",
        "      a = driver.find_elements_by_xpath('//*[@id=\"root\"]/div/div/section[3]/div/div/div[2]/div/div['+str(j)+']/div[1]/div[1]/div[1]/img')\n",
        "      a_num=0\n",
        "      for k in range(5):\n",
        "        if a[k].get_attribute('src')=='https://klue.kr/images/orange-pow.svg':\n",
        "          a_num = a_num+1\n",
        "\n",
        "      b = driver.find_element_by_xpath('/html/body/div[1]/div/div/section[3]/div/div/div[2]/div/div['+str(j)+']/div[1]/div[1]/div[2]/div[1]/span').text\n",
        "      b_num = int(b)\n",
        "\n",
        "      c = driver.find_element_by_xpath('/html/body/div[1]/div/div/section[3]/div/div/div[2]/div/div['+str(j)+']/div[1]/div[1]/div[2]/div[2]/span').text\n",
        "      c_num = int(c)\n",
        "\n",
        "      d = driver.find_element_by_xpath('/html/body/div[1]/div/div/section[3]/div/div/div[2]/div/div['+str(j)+']/div[1]/div[1]/div[2]/div[3]/span').text\n",
        "      d_num = int(d)\n",
        "\n",
        "      e = driver.find_element_by_xpath('/html/body/div[1]/div/div/section[3]/div/div/div[2]/div/div['+str(j)+']/div[1]/div[1]/div[2]/div[4]/span').text\n",
        "      e_num = int(e)\n",
        "\n",
        "      f = driver.find_element_by_xpath('/html/body/div[1]/div/div/section[3]/div/div/div[2]/div/div['+str(j)+']/p').text\n",
        "\n",
        "      g = driver.find_element_by_xpath('/html/body/div[1]/div/div/section[3]/div/div/div[2]/div/div['+str(j)+']/div[2]/span[1]')\n",
