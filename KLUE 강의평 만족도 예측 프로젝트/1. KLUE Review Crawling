        "## 2020년 2학기 STAT 과목 링크 찾기\n"
      ]
    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "XPWhe2UC18Wc"
      },
      "source": [
        "### 통계학과 링크 크롤링 --> 표로 정리"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "H7f64DQud-b-"
      },
      "source": [
        "semesters = []\n",
        "links = []\n",
        "\n",
        "URL = \"https://klue.kr/search?query=stat&sort=year_term\"\n",
        "driver.get(url=URL)\n",
        "\n",
        "i=0\n",
        "while i <100:\n",
        "  try:\n",
        "    i = i+1\n",
        "    semester = driver.find_element_by_xpath('/html/body/div/div/div/section[2]/div/div/ul/div/div/li['+str(i)+']/a/div[1]/p[1]')\n",
        "    a = semester.text\n",
        "    semesters.append(a)\n",
        "    link = driver.find_element_by_xpath('/html/body/div/div/div/section[2]/div/div/ul/div/div/li['+str(i)+']/a')\n",
        "    b = link.get_attribute('href')\n",
        "    links.append(b)\n",
        "    if '2020년 여름학기' in a:\n",
        "      break\n",
        "  except:\n",
        "    body = driver.find_element_by_css_selector('body')\n",
        "    body.send_keys(Keys.PAGE_DOWN)\n",
        "    time.sleep(2)\n",
        "    i = i-1\n"
      ],
      "execution_count": null,
      "outputs": []
    },
